
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'myddns_ipv4'
	option service_name 'dynv6.com'
	option lookup_host '85631065.dynv6.net'
	option enabled '1'
	option use_ipv6 '0'
	option domain '85631065.dynv6.net'
	option username 'none'
	option password '9KaAFqa7seePv8SZD8LgQ4riRAXxJ9'
	option ip_source 'web'
	option ip_url 'http://checkip.dyndns.com'
	option interface 'wan'
	option use_syslog '2'
	option check_unit 'minutes'
	option force_unit 'minutes'
	option retry_unit 'seconds'

config service 'myddns_ipv6'
	option lookup_host '85631065.dynv6.net'
	option use_ipv6 '1'
	option enabled '1'
	option service_name 'dynv6.com'
	option domain '85631065.dynv6.net'
	option username 'none'
	option password '9KaAFqa7seePv8SZD8LgQ4riRAXxJ9'
	option ip_source 'web'
	option ip_url 'http://checkipv6.dyndns.com'
	option interface 'wan'
	option use_syslog '2'
	option check_unit 'minutes'
	option force_unit 'minutes'
	option retry_unit 'seconds'

